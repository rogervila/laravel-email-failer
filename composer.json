{
    "name": "rogervila/laravel-email-failer",
    "description": "Helper class for testing Laravel Mail Failures",
    "keywords": [
        "laravel mail failure",
        "laravel mail error"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roger Vil√†",
            "email": "rogervila@me.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "LaravelEmailFailer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "laravel/framework": "5.8.*|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.0 || ^9.0",
        "orchestra/testbench": "^3 || ^4 || ^5 || ^6",
        "rogervila/php-sonarqube-scanner": "^1.0"
    }
}
